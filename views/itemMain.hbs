<div class="content_50">
    <input type="hidden" value="{{pageObject}}" name="objectType" id="objectType" />
   {{#if breadcrumbs}}
       
        {{#if breadcrumbs.sub}}
        <button class="btnGreyBorder back_blue center shadow_box" style="width: 49%;" onclick="location.href='/{{url breadcrumbs.main}}/{{url breadcrumbs.sub}}';">{{ucwords breadcrumbs.sub}}</button>
        {{/if}}
    {{/if}} 
    <div class="clear"></div>
</div>
<div class="content_50">
   <button class="btnGreyBorder back_form shadow_box " style="width: 49%;float:left" >Member Collections</button>
    <button class="btnGreyBorder back_form shadow_box btnSearch" style="width: 49%;float:right" >Search Options</button>
</div>
<div class="clear"></div>
<div style="display: none;" class="search_box" id="search_box">
    <div class="content_50 back_secondary shadow_box">
        <div class="in">

            <!--Platform-->
            <h5 class="left">Platform</h5>
            <select aria-label="Section" style="width: 100%;" name="search_section" autocomplete="off" class="form_select back_form">
                {{{searchOptions}}}
            </select>

            <!--END OF PLATFORM-->
            <h5 class="left">Sort By</h5>
            <select class="form_select back_form" style="width: 100%;">
                <option value="name">Name</option>
                <option value="comp">Finished</option>
                <option value="averagea">Region</option>
                <option value="release">Release Date</option>
                {{#ifEquals pageObject 'games'}}
                    <option value="playmode">Playmode</option>
                {{/ifEquals}}   
            </select>
        </div>
    </div>
    <div class="content_50 back_secondary shadow_box">
        <div class="in">
            <h5 class="left">Genre</h5>

            <!--Genre-->
            <select name="search_genre" style="width: 100%;" class="form_select back_form">
                <option value="">All</option>
                 {{#each searchGenre}}
                    <option value="{{ucwords this.genre}}">{{ucwords this.genre}}</option>
                {{/each}}
            </select>

            <!--END OF GENRE-->
            <div class="clear"></div>
            <h5 class="left">Release Year</h5>
            <select name="search_year" style="width: 100%;" class="form_select back_form form_select back_form"
                data-form-type="date,year">
                <option value="">All </option>
                {{#each searchYears}}
                    <option value="{{this}}">{{this}}</option>
                {{/each}}
            </select>
            <div class="clear"></div>
        </div>
    </div>
    <div class="clear"></div>
</div>
<div class="content_100">
    <div class="in">
        <h1 class="text_orange">{{ucwords datapercent.title}} Collection</h1>
        <div class="search_list">
            <ul>
                <li class="back_darkish Card"
                    style="background-image: linear-gradient(rgb(31, 31, 31), rgba(31, 31, 31, 0.85)), url(&quot;/img/backimg.png&quot;);">
                    <div class="Card_Blur">
                        <div class="Card_Details" style="margin-left: 0px;">
                            <h3><a class="text_white" title="{{ucwords datapercent.main}} Grand Total"
                                    href="#">{{ucwords datapercent.main}}
                                    Grand Total</a> </h3>
                            <div class="Card_Detail_Block">
                                <div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="usa"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            USA: {{datapercent.region.usa}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="japan"></div>
                                        <div style="margin-top: 10px; margin-left: 50px;">
                                            Japan: {{datapercent.region.japan}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="europe"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            Europe: {{datapercent.region.europe}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="asia"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            Asia: {{datapercent.region.asia}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="korea"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            Korea: {{datapercent.region.korea}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="australia"></div>
                                        <div style="margin-top: 10px; margin-left: 50px;">
                                            Australia: {{datapercent.region.australia}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item center time_100">
                                        Total: {{datapercent.total}}
                                    </div>
                                    <div class="Card_List_Item center time_100">
                                        Price: ${{datapercent.price}}
                                    </div>
                                    <div class="clear"></div>
                                </div>
                            </div>
                            <div class="progress_bar">
                                <div role="progressbar" style="width: {{datapercent.percentage}}%;" aria-valuenow="3"
                                    aria-valuemin="0" aria-valuemax="128">
                                </div>
                            </div>
                            <div style="font-size: 10px;text-align: center;">{{datapercent.percentage}}%</div>
                            <div style="font-size: 10px;text-align: center;font-size: 14px;">
                                Finished
                            </div>
                        </div>
                    </div>
                </li>
               

                {{#each data}}
                {{#if this.brand}}
                <li class="back_darkish Card"
                    onclick="location.href='/{{url this.main}}/{{url this.brand}}/{{{url this.name}}}';"
                    style="background-image: linear-gradient(rgb(31, 31, 31), rgba(31, 31, 31, 0.85)), url(&quot/img/backimg.png&quot;);">
                    {{else}}
                <li class="back_darkish Card" onclick="location.href='/{{url this.main}}/{{{url this.name}}}';"
                    style="background-image: linear-gradient(rgb(31, 31, 31), rgba(31, 31, 31, 0.85)), url(&quot/img/backimg.png&quot;);">
                    {{/if}}

                    <div class="Card_Blur">
                        <div class="Card_Details" style="margin-left: 0px;">
                            <h3><a class="text_white" title="Games Grand Total" href="#">{{this.name}}</a> </h3>
                            <div class="Card_Detail_Block">
                                <div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="usa"></div>
                                        <div style="margin-top: 10px; margin-left: 50px;">
                                            USA: {{this.region.usa}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="japan"></div>
                                        <div style="margin-top: 10px; margin-left: 50px;">Japan:
                                            {{this.region.japan}}</div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="europe"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">Europe:
                                            {{this.region.europe}}</div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="asia"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">Asia: {{this.region.asia}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="korea"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">Korea:
                                            {{this.region.korea}}</div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="australia"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            Australia: {{this.region.australia}}</div>
                                    </div>
                                    <div class="Card_List_Item center time_100">
                                        Total: {{this.total}}
                                    </div>
                                    <div class="Card_List_Item center time_100">
                                        Price: ${{this.price}}
                                    </div>
                                    <div class="clear"></div>
                                </div>
                            </div>
                            <div class="progress_bar">
                                <div role="progressbar" style="width: {{this.percentage}}%;" aria-valuenow="3"
                                    aria-valuemin="0" aria-valuemax="128">
                                </div>
                            </div>
                            <div style="font-size: 10px;text-align: center;"> {{this.percentage}}%</div>
                            <div style="font-size: 10px;text-align: center;font-size: 14px;">
                                Finished</div>
                        </div>
                    </div>
                </li>
                {{/each}}

                <div class="clear"></div>
            </ul>
        </div>
        <div class="clear"></div>
    </div>
</div>