<div class="content_50">
    <input type="hidden" value="{{pageObject}}" name="objectType" id="objectType" />
    {{#if breadcrumbs}}
        {{#if breadcrumbs.sub}}
        <button class="btnGreyBorder back_blue center shadow_box" style="width: 49%;"
            onclick="location.href='/{{url breadcrumbs.main}}/{{url breadcrumbs.sub}}';">{{ucwords
            breadcrumbs.sub}}</button>
        {{/if}}
    {{/if}}
    <div class="clear"></div>
</div>

<div class="content_50">
    <button class="btnGreyBorder back_form shadow_box " style="width: 49%;float:left">Member Collections</button>
    <button class="btnGreyBorder back_form shadow_box btnSearch" style="width: 49%;float:right">Search Options</button>
</div>
<div class="clear"></div>
<div style="display: none;" class="search_box" id="search_box">
    <div class="content_50 back_secondary shadow_box">
        <div class="in">
            <h5 class="left">Platform</h5>
            <select aria-label="Sections" style="width: 100%;" name="search_section" autocomplete="off"
                class="form_select back_form">
                {{{searchOptions}}}
            </select>
            <h5 class="left">Sort By</h5>
            <select class="form_select back_form" style="width: 100%;">
                <option value="name">Name</option>
                <option value="comp">Finished</option>
                <option value="averagea">Region</option>
                <option value="release">Release Date</option>
                {{#ifEquals pageObject 'games'}}
                    <option value="playmode">Playmode</option>
                {{/ifEquals}}   
            </select>
        </div>
    </div>
    <div class="content_50 back_secondary shadow_box">
        <div class="in">
            <h5 class="left">Genre</h5>
            <!--Genre-->
            <select name="search_genre" style="width: 100%;" class="form_select back_form">
                <option value="">All</option>
                {{#each searchGenre}}
                <option value="{{ucwords this.genre}}">{{ucwords this.genre}}</option>
                {{/each}}
            </select>

            <!--END OF GENRE-->
            <div class="clear"></div>
            <h5 class="left">Release Year</h5>
            <select name="search_year" style="width: 100%;" class="form_select back_form form_select back_form"
                data-form-type="date,year">
                <option value="">All </option>
                {{#each searchYears}}
                <option value="{{this}}">{{this}}</option>
                {{/each}}
            </select>
            <div class="clear"></div>
        </div>
    </div>
    <div class="clear"></div>
</div>
<div class="content_100">
    <div class="in">
        <div class="search_list">
            <ul>

                <li class="back_darkish Card"
                    style="width:100%;background-image: linear-gradient(rgb(31, 31, 31), rgba(31, 31, 31, 0.85)), url(/img/backimg.png);">
                    <div class="Card_Blur">
                        <div class="Card_Details" style="margin-left: 0px;">
                            <h3><a class="text_white" title="Games Grand Total" href="#">{{datapercent.type}}
                                    Grand Total</a> </h3>
                            <div class="Card_Detail_Block">
                                <div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="usa"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            USA: {{datapercent.usa}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="japan"></div>
                                        <div style="margin-top: 10px; margin-left: 50px;">
                                            Japan: {{datapercent.japan}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="europe"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            Europe: {{datapercent.europe}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="asia"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            Asia: {{datapercent.asia}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="korea"></div>
                                        <div style="margin-top: 10px;margin-left: 50px;">
                                            Korea: {{datapercent.korea}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item text_white shadow_text">
                                        <div class="australia"></div>
                                        <div style="margin-top: 10px; margin-left: 50px;">
                                            Australia: {{datapercent.australia}}
                                        </div>
                                    </div>
                                    <div class="Card_List_Item center time_100">
                                        Total: {{datapercent.total}}
                                    </div>
                                    <div class="Card_List_Item center time_100">
                                        Price: ${{datapercent.cost}}
                                    </div>
                                    <div class="clear"></div>
                                </div>
                            </div>
                            <div class="progress_bar">
                                <div role="progressbar" style="width:  {{datapercent.percentage}}%;" aria-valuenow="3"
                                    aria-valuemin="0" aria-valuemax="128">
                                </div>
                            </div>
                            <div style="font-size: 10px;text-align: center;"> {{datapercent.percentage}}%</div>
                            <div style="font-size: 10px;text-align: center;font-size: 14px;">
                                Finished
                            </div>
                        </div>
                    </div>
                </li>

                <div class="clear"></div>
            </ul>
        </div>
        <div class="clear"></div>
    </div>
</div>
<div class="content_100 back_form shadow_box">
    <div class="in">

        <div class="search_list">
            <table class="table table-striped table-hover r-0"
                style="width:100%;border-spacing: 0px 0; border-collapse: separate;">
                <thead>
                    <tr style="text-align: left;padding:5px;">
                        <th>NAME</th>
                        <th>STATUS</th>
                    </tr>
                </thead>

                <tbody>
                    {{#each data}}
                    {{#if this.nestedsub}}
                    <tr style="cursor:pointer;"
                        onclick="location.href='/{{url this.main}}/{{url this.sub}}/{{url this.nestedsub}}/{{this.id}}/view';">
                        {{else}}
                    <tr style="cursor:pointer;"
                        onclick="location.href='/{{url this.main}}/{{url this.sub}}/{{this.id}}/view';">
                        {{/if}}
                        <td style="padding:5px 0;">
                            <div class="avatar-md mr-3 mt-1 float-left">
                                <div class="{{this.region_icon}}"></div>
                            </div>
                            <div style="min-height: 40px;margin-left:50px;">
                                <div>
                                    <strong>{{this.name}}</strong><br>
                                    <small>{{this.release_date}}</small>
                                </div>

                            </div>
                        </td>
                        <td style="padding:5px 0;">
                            {{#ifEquals this.finished 'completed'}}
                            <span class="icon icon-circle s-12  mr-2  text-success"></span>Completed
                            {{/ifEquals}}
                            {{#ifEquals this.finished 'pending'}}
                            <span class="icon icon-circle s-12  mr-2  text-danger"></span>Pending
                            {{/ifEquals}}
                        </td>
                    </tr>
                    {{/each}}
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>